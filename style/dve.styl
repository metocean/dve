.dve-container

  line
    stroke black

  // text
  //   fill rgb(20, 44, 88)

  // svg
  //   border 1px solid rgb(213, 226, 237)

  svg.space
    border none

  path
    fill none
    stroke black
    &.primary
      stroke rgb(20, 44, 88)
      stroke-width 2

    &.secondary
      stroke-dasharray 10, 10
      stroke rgb(20, 44, 88)
      stroke-width 2
    .arrowhead
      fill rgb(20, 44, 88)
      stroke rgb(20, 44, 88)

  .arrowline
    stroke-width 3
    stroke rgb(20, 44, 88)

    rect.background
      fill rgb(43, 108, 167)

    rect.foreground
      fill rgba(0, 0, 0, 0)
      -webkit-touch-callout none
      -webkit-user-select none
      -khtml-user-select none
      -moz-user-select none
      -ms-user-select none
      user-select none
      cursor ew-resize
      cursor col-resize

    .line, .sep
      stroke white

  line.poi
    stroke rgb(216, 34, 42)
    stroke-width 0.75

  .inner .divider
    stroke rgb(213, 226, 237)
    shape-rendering crispEdges

  .axis
    line
      shape-rendering crispEdges
      stroke rgb(213, 226, 237)
      stroke-width 2

    path
      shape-rendering crispEdges
      stroke rgb(213, 226, 237)
      stroke-width 1

  .inner
    pointer-events all

  .inner .x.axis .tick
    line.major
      shape-rendering crispEdges
      stroke rgb(213, 226, 237)
      stroke-width 2

    line.minor
      shape-rendering crispEdges
      stroke rgba(213, 226, 237, 0.5)

    line.sub-minor
      stroke none

  .axis.y .tick line
    shape-rendering crispEdges
    stroke-dasharray 10, 10
    stroke rgba(42, 108, 167, 0.2)
    stroke-width 1

    &.zero
      stroke rgb(20, 44, 88)
      stroke-dasharray none

  .poi-circle
    fill rgb(216, 34, 42)

  .infotext
    font-size 17px

  .label-shad,.poi-y-val-shad
    stroke white
    stroke-width 3.5
    opacity 0.9
    font-size 12px

  .arrow-background
    stroke rgba(216, 34, 42, 0.5)
    stroke-width 1
    fill rgba(255,255,255, 0.9)

  .section .label
    font-size 12px

  .focus .label, .poi-y-val
    fill rgb(216, 34, 42)
    font-size 14px
    font-weight 600

  .poi-y-val-shad
    stroke white
    stroke-width 5.75
    font-size 16px
    opacity 0.9

  .foc-section .arrowhead, .foc-section .arrowline
    stroke rgb(216, 34, 42)
    fill rgb(216, 34, 42)

  .dot
    fill rgb(20, 44, 88)

  .windrose
    path
      shape-rendering inherit
      fill currentColor
      stroke none

    .circlecontainer circle
      stroke-dasharray 5, 5
      fill none
      stroke rgb(43, 108, 167)
      stroke-width 0.5
    .spoke
      stroke-dasharray 5, 5
      stroke-width 1
      stroke rgba(43, 108, 167, 0.45)

  .histogram
    .bar
      fill rgb(20, 44, 88)
      shape-rendering crispEdges
      stroke none

  .table, .tablebytime
    .inner
      vertical-align middle
      background rgba(255,255,255, 0.2)
      box-shadow inset 0 0 3px 0px #CECECE

    .header rect
      fill none

    .header text
      font 10px sans-serif
      text-anchor middle
      font-weight bold
    .header.side text
      text-anchor start

    .cell text
      font 10px sans-serif
      text-anchor middle
      // font-weight bold

  .tick text
  .axis g text
  .circlecontainer text
  .legend text
    font 10px sans-serif
    font-weight bold
    